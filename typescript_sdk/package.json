{
  "name": "@rileyseaburg/tiktok-business-api-sdk",
  "version": "2.0.0",
  "description": "Modern TypeScript SDK for TikTok Business API built with ORPC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "npx orpc generate",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "echo 'Tests will run after npm install. Run: node bootstrap.js first'",
    "test:coverage": "echo 'Coverage will run after npm install. Run: node bootstrap.js first'",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run type-check && npm run lint",
    "release": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "tiktok",
    "business-api",
    "typescript",
    "sdk",
    "orpc",
    "type-safe"
  ],
  "author": "TikTok Business API Team",
  "license": "MIT",
  "dependencies": {
    "@orpc/client": "^1.8.9",
    "@orpc/contract": "^1.8.9",
    "@orpc/server": "^1.8.9",
    "@orpc/zod": "^1.8.9",
    "fetch-retry": "^6.0.0",
    "zod": "4.1.9"
  },
  "devDependencies": {
    "@orpc/openapi": "^1.8.9",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.36.0",
    "rimraf": "6.0.1",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rileyseaburg/tiktok-business-api-sdk.git",
    "directory": "typescript_sdk"
  }
}
